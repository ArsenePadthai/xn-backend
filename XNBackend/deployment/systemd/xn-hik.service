[Unit]
Description=Restart celery worker1

[Service]
Type=simple
User=ubuntu
WorkingDirectory=/home/ubuntu/xn-backend
Environment=XN_SETTINGS=../settings.cfg
ExecStart=/home/ubuntu/.local/share/virtualenvs/xn-backend-InHQO0Yz/bin/celery -A XNBackend.app worker --concurrency=10 -n worker1@%n -Q first
Restart=always
RestartSec=100ms

[Install]
WantedBy=multi-user.target
