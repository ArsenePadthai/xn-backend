[Unit]
Description=Restart celery worker 

[Service]
Type=simple
User=ubuntu
WorkingDirectory=/home/ubuntu/xn-backend
Environment=XN_SETTINGS=../settings.cfg
ExecStart=/home/ubuntu/.local/share/virtualenvs/xn-backend-InHQO0Yz/bin/celery -A XNBackend.app worker -B -n worker@%i -Q %i
Restart=always
RestartSec=100ms

[Install]
WantedBy=multi-user.target
